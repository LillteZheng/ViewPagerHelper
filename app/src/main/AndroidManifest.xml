<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhengsr.viewpagerhelper">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.glide.GlideZoomActivity" />
        <activity android:name=".activity.glide.GlideTransActivity" />
        <activity android:name=".activity.glide.GlidenormalActivity" />
        <activity android:name=".LoopActivity" />
        <activity android:name=".tab.TabActivity"></activity>
        <activity android:name=".tab.TriTabActivity"></activity>
        <activity android:name=".tab.RectTabActivity"></activity>
        <activity android:name=".tab.ColorTabActivity"></activity>
    </application>

</manifest>